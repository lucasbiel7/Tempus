<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.QuadroDeHorario.view.LixeiraUsuarioController">
    <stylesheets>
        <URL value="@estilo.css" />
    </stylesheets>
    <children>
        <TableView fx:id="tvUsuario" layoutX="393.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="tcNome" prefWidth="75.0" text="Nome" />
                <TableColumn fx:id="tcEmail" prefWidth="75.0" text="E-mail" />
                <TableColumn fx:id="tcLogin" prefWidth="75.0" text="Login" />
                <TableColumn fx:id="tcGrupo" prefWidth="75.0" text="Grupo" />
                <TableColumn fx:id="tcTelefone" prefWidth="75.0" text="Telefone" />
                <TableColumn fx:id="tcCelular" prefWidth="75.0" text="Celular" />
                <TableColumn fx:id="tcEndereco" prefWidth="75.0" text="Endereço" />
                <TableColumn fx:id="tcCargaHoraria" prefWidth="75.0" text="Carga Horária&#10;Diária" />
            </columns>
            <contextMenu>
                <ContextMenu>
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#btRestaurarActionEvent" text="Restaurar" />
                        <MenuItem mnemonicParsing="false" onAction="#miExcluirActionEvent" text="Excluir permanentemente" />
                    </items>
                </ContextMenu>
            </contextMenu>
        </TableView>
    </children>
</AnchorPane>
